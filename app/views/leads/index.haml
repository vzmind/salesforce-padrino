.block
  .secondary-navigation
    %ul.wat-cf
      %li "oto"
  .content
    %h2.title
      Leads
    .inner
      %table.table
        %tr
          %th.first="ID"
          %th="Name"
          %th.last="Annual Revenue"
        -@leads.each do |lead|
          %tr{:onclick=>"window.location='index.html'"}
            %td.first=lead["Id"]
            %td=lead["Name"]
            %td.last
              =lead["AnnualRevenue"]
      .actions-bar.wat-cf
        .actions="&nbsp;"

-content_for :sidebar, partial("base/sidebar")
